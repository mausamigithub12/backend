      "@types/swagger-jsdoc": "^6.0.4",
      "@types/swagger-ui-express": "^4.1.6",
      "nodemon": "^3.0.3",
      "ts-node": "10.7.0",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "@upstash/redis": "^1.28.3",
      "axios": "^1.6.7",
      "bcryptjs": "^2.4.3",
      "body-parser": "^1.19.1",
      "class-validator": "^0.14.1",
      "cookie-parser": "^1.4.6",
      "cors": "^2.8.5",
      "dotenv": "^16.3.1",
      "ejs": "^3.1.9",
